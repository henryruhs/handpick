{
	"config": ".handpick",
	"manager": "npm",
	"managerObject":
	{
		"npm":
		[
			"install",
			"--no-package-lock"
		],
		"pnpm":
		[
			"install",
			"--no-lockfile"
		],
		"yarn":
		[
			"--no-lockfile"
		]
	},
	"range": "dirty",
	"rangeArray":
	[
		"dirty",
		"exact",
		"patch",
		"minor"
	],
	"ignorePrefix": "__",
	"ignoreArray":
	[
		"dependencies",
		"devDependencies"
	],
	"targetArray":
	[
		"dependencies",
		"devDependencies"
	],
	"filterArray": [],
	"referencePrefix": "@",
	"spinnerTime": 40,
	"spinnerArray":
	[
		"⠋",
		"⠙",
		"⠹",
		"⠸",
		"⠼",
		"⠴",
		"⠦",
		"⠧",
		"⠇",
		"⠏"
	],
	"path": ".",
	"packageFile": "package.json",
	"packageDirectory": "node_modules"
}
